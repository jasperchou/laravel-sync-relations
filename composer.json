{
    "name": "routegroup/laravel-sync-relations",
    "description": "Sync relationships for Eloquent",
    "keywords": [
        "routegroup",
        "native-framework",
        "relations",
        "sync",
        "Eloquent"
    ],
    "homepage": "https://github.com/routegroup/laravel-sync-relations",
    "license": "MIT",
    "authors": [
        {
            "name": "Kajetan Nobel",
            "email": "k.nobel@routegroup.pl",
            "homepage": "http://routegroup.pl",
