        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "illuminate/support": "~5",
        "illuminate/database": "~5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.4",
        "orchestra/database": "3.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Routegroup\\Database\\Relations\\": "src"
        }
